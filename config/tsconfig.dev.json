{
  "compilerOptions": {
    "baseUrl": "../",
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": true,
    "jsx": "react",
    "lib": ["dom", "es2017"],
    "strict": false,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false
  }
}
