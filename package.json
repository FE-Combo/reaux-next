{
  "name": "reaux-next",
  "version": "0.2.2",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "reinstall": "rm -rf node_modules && rm -rf yarn.lock && rm -rf .next && yarn install",
    "dev": "yarn build:lib && next dev",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "build:lib": "rm -rf dist && tsc -p config/tsconfig.build.json",
    "locales": "node ./node_modules/dot-i18n/node/createLocale",
    "lint": "next lint",
    "prettier": "prettier --write .",
    "type-check": "tsc --pretty --noEmit",
    "release": "standard-version",
    "release:beta": "standard-version --prerelease beta",
    "release:alpha": "standard-version --prerelease alpha",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(tsx|ts)": [
      "yarn lint",
      "yarn prettier",
      "git add"
    ],
    "*.@(js|json)": [
      "yarn prettier",
      "git add"
    ],
    "*.@(scss)": [
      "yarn stylelint",
      "yarn prettier",
      "git add"
    ]
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "author": "Zed",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.1.0",
    "antd": "^5.26.7",
    "axios": "^1.11.0",
    "dot-i18n": "^1.1.0",
    "encodeurl": "^2.0.0",
    "typescript-eslint": "^8.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "next": "^15.4.5",
    "prettier": "^3.6.2",
    "@types/react": "~19.1.9",
    "@types/react-dom": "~19.1.7",
    "react": "~19.1.1",
    "react-dom": "~19.1.1",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "@eslint/eslintrc": "^3",
    "js-cookie": "^3.0.5",
    "@types/js-cookie": "^3.0.6"
  },
  "dependencies": {
    "@types/redux-logger": "~3.0.13",
    "connected-next-router": "~5.0.0",
    "react-intersection-observer": "~9.16.0",
    "@redux-devtools/extension": "~3.3.0",
    "redux-logger": "~3.0.6",
    "reaux": "~2.5.1"
  },
  "peerDependencies": {
    "@types/redux-logger": "~3.0.13",
    "connected-next-router": "~5.0.0",
    "react-intersection-observer": "^9.16.0",
    "@redux-devtools/extension": "~3.3.0",
    "redux-logger": "~3.0.6",
    "reaux": "~2.5.1"
  }
}